{
  "name": "fragments",
  "private": true,
  "version": "0.11.1",
  "description": "Fragments back-end API",
  "scripts": {
    "test:watch": "jest -c jest.config.js --runInBand --watch --",
    "test": "jest -c jest.config.js --runInBand --",
    "test:openhandles": "jest -c jest.config.js --runInBand --detectOpenHandles --",
    "test:integration": "hurl --test --jobs 1 --glob \"tests/integration/**/*.hurl\"",
    "coverage": "jest -c jest.config.js --runInBand --coverage",
    "lint": "eslint \"./src/**/*.js\" \"tests/**/*.js\"",
    "start": "node src/index.js",
    "dev": "cross-env LOG_LEVEL=debug nodemon ./src/index.js --watch src",
    "debug": "cross-env LOG_LEVEL=debug nodemon --inspect=0.0.0.0:9229 ./src/index.js --watch src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LucaCasola/fragments.git"
  },
  "keywords": [],
  "author": "Luca Casola",
  "license": "UNLICENSED",
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@orangeopensource/hurl": "^6.1.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.26.0",
    "globals": "^16.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "prettier": "3.5.3",
    "supertest": "^7.1.1"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.858.0",
    "@aws-sdk/client-s3": "^3.855.0",
    "@aws-sdk/lib-dynamodb": "^3.858.0",
    "aws-jwt-verify": "^5.1.0",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "http-auth": "^4.2.1",
    "http-auth-passport": "^1.0.7",
    "markdown-it": "^14.1.0",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "sharp": "^0.34.3",
    "stoppable": "^1.1.0"
  }
}
